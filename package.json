{
  "name": "Google-Keep-2.0",
  "version": "1.0.0",
  "description": "Keep notes app",
  "keywords": [],
  "main": "src/index.js",
  "engines": {
    "node": "12.16.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.10"
  },
  "devDependencies": {
    "typescript": "3.3.3"
  },
  "scripts": {
    "client-install": "cd client && npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
